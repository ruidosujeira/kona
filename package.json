{
  "author": "Kona Contributors",
  "bugs": {
    "url": "https://github.com/ruidosujeira/kona/issues"
  },
  "collective": {
    "logo": "https://opencollective.com/opencollective/logo.txt",
    "type": "opencollective",
    "url": "https://opencollective.com/kona"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "helpMessage": "Valid commit message formats are: \n - <type>(<scope>): <subject> \n - <type>: <subject>",
      "scope": {
        "allowed": [
          "*"
        ],
        "multiple": false,
        "required": false,
        "validate": false
      }
    }
  },
  "dependencies": {
    "@typescript-eslint/typescript-estree": "^8.18.0",
    "app-root-path": "^3.1.0",
    "astring": "^1.9.0",
    "base64-js": "^1.5.1",
    "chokidar": "^4.0.3",
    "clean-css": "^5.3.3",
    "comment-json": "^4.2.5",
    "convert-source-map": "^2.0.0",
    "express": "^5.0.1",
    "fs-extra": "^11.2.0",
    "fuse-concat-with-sourcemaps": "^1.0.5",
    "get-port": "^7.1.0",
    "glob": "^11.0.0",
    "http-proxy-middleware": "^3.0.3",
    "ieee754": "^1.2.1",
    "lego-api": "^1.0.8",
    "lodash.merge": "^4.6.2",
    "meriyah": "^6.0.3",
    "offset-sourcemap-lines": "^1.0.1",
    "open": "^10.1.0",
    "postcss": "^8.4.49",
    "postcss-import": "^16.1.0",
    "pretty-time": "^1.1.0",
    "sass": "^1.82.0",
    "source-map": "^0.7.4",
    "source-map-support": "^0.5.21",
    "stream-browserify": "^3.0.0",
    "terser": "^5.37.0",
    "typescript": "^5.7.2",
    "ws": "^8.18.0"
  },
  "description": "The fastest JavaScript bundler. Faster than esbuild. Written in Rust + TypeScript.",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/lodash.merge": "^4.6.9",
    "@types/node": "^22.10.1",
    "@types/styled-components": "^5.1.34",
    "codecov": "^3.8.3",
    "commitizen": "^4.3.1",
    "consolidate": "^1.0.3",
    "conventional-changelog-cli": "^5.0.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "electron": "^33.2.1",
    "electron-settings": "^4.0.4",
    "fuse-box-typechecker": "^3.0.1",
    "hammerjs": "^2.0.8",
    "homedir": "^0.6.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jsdom": "^25.0.1",
    "less": "^4.2.1",
    "lodash-es": "^4.17.21",
    "marked": "^15.0.3",
    "minify-html-literals": "^1.3.5",
    "moment": "^2.30.1",
    "npm-run-all": "^4.1.5",
    "postcss-modules": "^6.0.1",
    "postcss-modules-scope": "^3.2.1",
    "postcss-selector-parser": "^7.0.0",
    "precss": "^4.0.0",
    "prettier": "^3.4.2",
    "pug": "^3.0.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "reflect-metadata": "^0.2.2",
    "run-sequence": "^2.2.1",
    "rxjs": "^7.8.1",
    "should": "^13.2.3",
    "styled-components": "^6.1.13",
    "stylus": "^0.64.0",
    "sucrase": "^3.35.0",
    "tinymce": "^7.6.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typedoc": "^0.27.4",
    "typedoc-plugin-markdown": "^4.4.1",
    "typescript-plugin-styled-components": "^3.0.0",
    "validate-commit-msg": "^2.14.0"
  },
  "engines": {
    "node": ">= 20",
    "npm": ">= 10.0.0"
  },
  "homepage": "https://github.com/ruidosujeira/kona#readme",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "rust-wasm/pkg",
    "README.md",
    "LICENSE"
  ],
  "name": "kona-bundler",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ruidosujeira/kona.git"
  },
  "bin": {
    "kona": "./dist/cli/cli.js"
  },
  "scripts": {
    "alpha": "ts-node -T kona publish-alpha",
    "build:wasm": "cd rust-wasm && wasm-pack build --target nodejs --out-dir pkg",
    "build:wasm:dev": "cd rust-wasm && wasm-pack build --target nodejs --dev --out-dir pkg",
    "build:wasm:release": "cd rust-wasm && wasm-pack build --target nodejs --release --out-dir pkg",
    "changelog": "run-s changelog:generate changelog:format",
    "changelog:format": "prettier --write CHANGELOG.md",
    "changelog:generate": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "cli": "ts-node -T src/cli/cli.ts",
    "codecov": "codecov",
    "commit": "git-cz",
    "commitmsg": "validate-commit-msg",
    "coverage": "jest --verbose --coverage",
    "coverage-ci": "jest --verbose --coverage --runInBand",
    "devDeps": "gulp installDevDeps",
    "dist": "ts-node -T kona dist",
    "dist:full": "run-s build:wasm:release dist docs",
    "docs": "typedoc --options typedoc.json",
    "docs:serve": "npx serve docs",
    "document": "ts-node -T kona document",
    "format": "prettier --list-different \"src/**/*.{js,json,md,ts,yml}\"",
    "format:fix": "prettier --write \"src/**/*.{js,json,md,ts,yml}\"",
    "next": "ts-node -T kona publish-next",
    "postinstall": "node -e \"try{require('./rust-wasm/pkg/kona_wasm.js')}catch(e){console.log('WASM module not built. Run npm run build:wasm for faster builds.')}\"",
    "test": "jest --verbose",
    "test-ci": "jest --verbose --runInBand",
    "test.watch": "jest --watch",
    "typecheck": "ts-node -T kona typecheck"
  },
    "version": "5.0.0"
}